[comment encoding = UTF-8 /]
[module generate('http://d.fe.up.pt/Jenkins')]

[import d::fe::up::pt::cicd::jenkins::acceleo::requests::utils/]

[template public generateElement(pipeline : Pipeline)]
[comment @main/]
[file ('Jenkinsfile', false, 'UTF-8')]
pipeline {
	stages {
	[for (stage : Stage | pipeline.stages)]
		stage([quoteString(stage.name)/]) {
			steps {
			[for (step : Step | stage.steps)]
				[step.command/]
			[/for]
			}
		}
	[/for]
	}
}
[/file]
[/template]
