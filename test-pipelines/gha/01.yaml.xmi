<?xml version="1.0" encoding="ASCII"?>
<GHA:Workflow xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:GHA="http://d.fe.up.pt/GHA">
  <name xsi:type="GHA:StringLiteral" value="Workflow"/>
  <runName xsi:type="GHA:Concat">
    <expressions xsi:type="GHA:StringLiteral" value="Deploy to "/>
    <expressions xsi:type="GHA:VariableDereference" property="deploy_target">
      <variable xsi:type="GHA:GitHubContext" context="inputs"/>
    </expressions>
    <expressions xsi:type="GHA:StringLiteral" value=" by @"/>
    <expressions xsi:type="GHA:VariableDereference" property="actor">
      <variable xsi:type="GHA:GitHubContext"/>
    </expressions>
  </runName>
  <triggers xsi:type="GHA:StandardEventTrigger" event="check_run">
    <eventTypes>assigned</eventTypes>
  </triggers>
  <triggers xsi:type="GHA:StandardEventTrigger"/>
  <triggers xsi:type="GHA:PullRequestTrigger">
    <paths xsi:type="GHA:StringLiteral" value="path"/>
  </triggers>
  <triggers xsi:type="GHA:PushTrigger">
    <tags xsi:type="GHA:StringLiteral" value="tag"/>
  </triggers>
  <triggers xsi:type="GHA:ScheduleTrigger">
    <crons xsi:type="GHA:StringLiteral" value="oooooo"/>
  </triggers>
  <triggers xsi:type="GHA:WorkflowCallTrigger">
    <inputs id="username">
      <description xsi:type="GHA:StringLiteral" value="A username passed from the caller workflow"/>
      <isRequired xsi:type="GHA:BooleanLiteral"/>
      <default xsi:type="GHA:StringLiteral" value="john-doe"/>
    </inputs>
    <outputs id="workflow_output1">
      <description xsi:type="GHA:StringLiteral" value="The first job output"/>
      <value xsi:type="GHA:VariableDereference" property="job_output1">
        <variable xsi:type="GHA:VariableDereference" property="outputs">
          <variable xsi:type="GHA:VariableDereference" property="my_job">
            <variable xsi:type="GHA:GitHubContext" context="jobs"/>
          </variable>
        </variable>
      </value>
    </outputs>
    <outputs id="workflow_output2">
      <description xsi:type="GHA:StringLiteral" value="The second job output"/>
      <value xsi:type="GHA:VariableDereference" property="job_output2">
        <variable xsi:type="GHA:VariableDereference" property="outputs">
          <variable xsi:type="GHA:VariableDereference" property="my_job">
            <variable xsi:type="GHA:GitHubContext" context="jobs"/>
          </variable>
        </variable>
      </value>
    </outputs>
    <secrets id="access-token">
      <description xsi:type="GHA:StringLiteral" value="A token passed from the caller workflow"/>
      <isRequired xsi:type="GHA:BooleanLiteral"/>
    </secrets>
  </triggers>
  <permissions/>
  <defaults>
    <shell xsi:type="GHA:StringLiteral" value="bash"/>
    <workingDirectory xsi:type="GHA:StringLiteral" value="./scripts"/>
  </defaults>
  <environmentVariables key="SERVER">
    <value xsi:type="GHA:StringLiteral" value="production"/>
  </environmentVariables>
  <concurrencyGroup>
    <name xsi:type="GHA:Concat">
      <expressions xsi:type="GHA:VariableDereference" property="workflow">
        <variable xsi:type="GHA:GitHubContext"/>
      </expressions>
      <expressions xsi:type="GHA:StringLiteral" value=" - "/>
      <expressions xsi:type="GHA:VariableDereference" property="ref">
        <variable xsi:type="GHA:GitHubContext"/>
      </expressions>
    </name>
    <cancelInProgress xsi:type="GHA:BooleanLiteral" value="true"/>
  </concurrencyGroup>
  <jobs xsi:type="GHA:ScriptJob" name="my_first_job">
    <jobName xsi:type="GHA:StringLiteral" value="My first job"/>
    <permissions/>
    <steps xsi:type="GHA:Command">
      <name xsi:type="GHA:StringLiteral" value="Print a greeting"/>
      <environmentVariables key="MY_VAR">
        <value xsi:type="GHA:StringLiteral" value="Hi there! My name is"/>
      </environmentVariables>
      <environmentVariables key="MIDDLE_NAME">
        <value xsi:type="GHA:StringLiteral" value="The"/>
      </environmentVariables>
      <environmentVariables key="LAST_NAME">
        <value xsi:type="GHA:StringLiteral" value="Octocat"/>
      </environmentVariables>
      <environmentVariables key="FIRST_NAME">
        <value xsi:type="GHA:StringLiteral" value="Mona"/>
      </environmentVariables>
      <command xsi:type="GHA:StringLiteral" value="echo 1"/>
    </steps>
  </jobs>
</GHA:Workflow>
